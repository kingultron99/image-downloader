{
  "name": "image-downloader",
  "version": "0.1.0",
  "homepage": "./",
  "private": true,
  "description": "An image scraper, presents the first 20 images straight from google images",
  "author": "kingultron99",
  "main": "src/electron-starter.js",
  "dependencies": {
    "@craco/craco": "^6.4.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.24.0",
    "cheerio": "^1.0.0-rc.10",
    "get-urls": "^10.0.1",
    "node-fetch": "^3.1.0",
    "puppeteer": "^11.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "read-excel-file": "^5.2.25",
